@model IEnumerable<WebStore.Domain.ViewModel.Product.BrandViewModel>
<div class="brands_products">
    <!--brands_products-->
    <h2>Бренды</h2>
    <div class="brands-name">
        <ul class="nav nav-pills nav-stacked">
            @foreach ( var brand in Model )
            {
                <li><a asp-action="Shop" asp-controller="Catalog" asp-route-brandId="@brand.Id"> @brand.Name</a></li>
            }
        </ul>
    </div>
</div><!--/brands_products-->
