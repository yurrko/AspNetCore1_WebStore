@{
    Layout = "_Layout";
}

@section top {
    @await RenderSectionAsync( "top" , false )
}

@section css {
    @await RenderSectionAsync( "css" , false )
}

@section scripts {
    @await RenderSectionAsync("scripts", false)
}


<div class="row">
    <div class="col-sm-3">
        @await Html.PartialAsync( "Partial/_LeftSideBar" )
    </div>

    <div class="col-sm-9 padding-right">
        @RenderBody()
    </div>
</div>
